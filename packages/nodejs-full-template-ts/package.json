{
  "name": "nodejs-full-template-ts",
  "version": "1.0.0",
  "description": "基于eggjs + prisma + mysql 的开发模板",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-nodejs-full-template-ts",
    "stop": "egg-scripts stop --title=egg-server-nodejs-full-template-ts",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "db:c": "npx prisma migrate dev --name db-change",
    "db:ui": "npx prisma studio",
    "db:pull": "npx prisma db pull",
    "lint": "eslint . --ext .ts --resolve-plugins-relative-to .",
    "clean": "ets clean"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "axios": "^1.2.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "egg": "^3.9.1",
    "egg-cors": "^2.2.3",
    "egg-router-plus": "^2.0.0",
    "egg-scripts": "^2.17.0",
    "jsonwebtoken": "^8.5.1",
    "koa-jwt": "^4.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^9.0.0",
    "autod": "^3.1.2",
    "autod-egg": "^1.1.0",
    "egg-bin": "^5.9.0",
    "egg-ci": "^2.1.0",
    "egg-mock": "^3.16.0",
    "eslint": "^8.0.0",
    "eslint-config-egg": "^12.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "ci": {
    "version": "14, 16, 18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MrGaoGang/frontend-tookit/packages/nodejs-full-template-ts"
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "mrgaogang",
  "license": "MIT"
}
